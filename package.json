{
  "description": "Boilerplate for setting up @fastify/vite with Typescript",
  "type": "module",
  "scripts": {
    "build": "npm run build:server && vite build",
    "build:server": "tsc",
    "dev": "npm run build:server && node dist/server.js --dev",
    "lint": "biome lint --write .",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "@fastify/vite": "8.0.0-alpha.2",
    "fastify": "^5.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/node": "^20.14.11",
    "@vitejs/plugin-react": "^4.3.1",
    "typescript": "^5.5.3",
    "vite": "^6.1.0"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@biomejs/biome"
    ],
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}